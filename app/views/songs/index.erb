<div class="container">
  <h1>All Songs: </h1>
  <ul>
  <% @songs.each do |song| %>
  <li>
    <a href="/songs/<%= song.id %>"><%= song.title %></a> by: <%= song.artist %>
  </li>
  <% end %>
  </ul>

  <form action="/songs" method="POST">
  	<input type="text" name="song[title]" />
  	<input type="submit" value="Search for Song"/> 
  </form>
</div>